#SEMC: TrimArea Daemon
type tad, domain;
type tad_exec, exec_type, file_type;
init_daemon_domain(tad)

allow tad mmc_block_device:blk_file { read write ioctl open };
allow tad modem_file:dir search;
allow tad self:capability dac_override;
allow tad self:process execmem;
allow tad socket_device:sock_file write;
